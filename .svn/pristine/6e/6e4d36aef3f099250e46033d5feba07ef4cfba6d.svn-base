	function setComponent(id, name, type, gatewayType)
	{
		alert( "setComponent:::" + id + "/" + name + "/" + type + "/" + gatewayType );
	}
	
	function dClickComponent(id, type)
	{
		alert( "dClickComponent:::" + id + "/" + type);
	}
	
	function setComponentName(id, name)
	{
		alert( "setComponentName:::" + id + "/" + name);
	}
	
	function dropComponent(id)
	{
		alert( "dropComponent:::" + id );
	}

	function dropComponents( obj )
	{
		var json = eval('(' + obj + ')');
		var str = "";
		for ( var i = 0; i < json.list.length; i++ )
		{
			str += "i = " + i + " / id = " + json.list[i].id  + " / type = " + json.list[i].type + "\n";
		}
		
		alert( "dropComponents:::\n" + str );
	}
	
	function setTransition(id, fromId, toId)
	{
		alert( "setTransition:::" + id + "/" + fromId + "/" + toId );
	}
	
	function dClickTransition(id)
	{
		alert( "dClickTransition:::" + id  );
	}
	
	function dropTransition(id)
	{
		alert( "dropTransition:::" + id  );
	}
	
	function setTransitionName(id, name)
	{
		alert( "setTransitionName:::" + id + "/" + name  );
	}
	
	function modifyTransition(id, fromId, toId)
	{
		alert( "modifyTransition:::" + id + "/" + fromId + "/" + toId );
	}
	
	function setProcessTitle(title)
	{
		alert( "setProcessTitle:::" + title  );
	}
	
	function thisMovie(movieName) 
	{
         if (navigator.appName.indexOf("Microsoft") != -1) 
             return window[movieName];
         else
             return document[movieName];
     }
     
     function sendToActionScript(value) 
     {
     	//alert(AppPrism);
     	
     	//value = 100;
        thisMovie("AppPrism").sendToActionScript(value);
     }
